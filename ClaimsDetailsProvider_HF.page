      function validateDate(){
      // if($(".endDate").val() != ""){
      var d = new Date();
      isValid=false;
      var currMonth = d.getMonth()+1
      var getCurrDate = currMonth+"/"+d.getDate()+"/"+d.getFullYear();

      // check if start date is greater than end date
      if(Date.parse($(".startDate").val()) > Date.parse($(".endDate").val())){
      /*if($(".startDate+.inlineErrorMessage").length!=0){
      $(".startDate+.inlineErrorMessage").remove();
      }*/
      $(".startDate").nextAll('.inlineErrorMessage').remove();
      var strMessageContainer = '<span class="inlineErrorMessage">Start date should be less than End Date</span>';
      $(".startDate").after(strMessageContainer);
      isValid=false;
      }
      else {
      $(".startDate").nextAll('.inlineErrorMessage').remove();
      isValid=true;
      }
      }

      /*
      * Method name: statusCheck
      * Description: assign class based on status field
      * Return Type: -
      * Parameter:   -
      */
      function statusCheck(){
      $(".label-default").each(function(strCount) {
      var strLabelTagValue = $(this).text();
      if(stringIsNotBlank(strLabelTagValue)){
      var strAddClass = '';
      var strRemoveClass = '';
      switch(strLabelTagValue.toUpperCase()) {
      case "OPEN":
      case "APPROVED":
      case "SUBMITTED":
      case "COMPLETED":
      case "ACTIVE":
      case "PAID":
      case "FINALIZED":
      case "YES":
      strAddClass = 'label-success';
      strRemoveClass = 'label-default';
      break;
      case "PENDING-RESOLUTION":
      case "PENDING":
      case "PENDING-ASSIGNMENT":
      strAddClass = 'label-warning';
      strRemoveClass = 'label-default';
      break;
      case "REJECTED":
      case "DENIED":
      case "RESOLVED-REJECTED":
      case "ESCALATED":
      case "NO":
      strAddClass = 'label-danger';
      strRemoveClass = 'label-default';
      break;
      case "ROUTED TO ANOTHER DEPARTMENT":
      case "PENDING-INFO":
      strAddClass = 'label-info';
      strRemoveClass = 'label-default';
      break;
      case "POSTED":
      // case "CLOSED":
      // case "INACTIVE":
      // case "CANCELLED":
      case "DRAFT":
      case "IN PROGRESS":
      case "RECEIVED":
      case "NOT STARTED":
      strAddClass = 'label-primary';
      strRemoveClass = 'label-default';
      break;
      }
      $(this).addClass(strAddClass);
      $(this).removeClass(strRemoveClass);
      }
      });
      };

